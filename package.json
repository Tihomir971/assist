    "@fontsource-variable/nunito": "^5.2.5",
    "@fontsource-variable/nunito-sans": "^5.2.5",
    "@fontsource-variable/oxanium": "^5.2.5",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.10",
    "@tailwindcss/vite": "4.1.8",
    "@tanstack/table-core": "^8.21.3",
    "@zag-js/avatar": "^1.15.0",
    "@zag-js/checkbox": "^1.15.0",
