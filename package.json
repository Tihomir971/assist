    "@tailwindcss/vite": "4.0.6",
    "@tanstack/table-core": "^8.20.5",
    "echarts": "^5.6.0",
    "html5-qrcode": "^2.3.8",
    "ky": "^1.7.4",
    "luxon": "^3.5.0",
    "melt": "^0.11.0",
    "tailwindcss": "4.0.6",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
