{
  "name": "assist",
  "version": "0.0.2",
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "22.13.1"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "update-deps": "pnpx npm-check-updates -i --format group --install always",
    "update-next": "pnpx npm-check-updates -i --format group --install always --target \"@next\"",
    "supabase-to-zod": "pnpm exec supabase-to-zod --input \"./src/lib/types/supabase.ts\" --output \"./src/lib/types/supabase-zod-schemas.ts\"",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "knip": "knip --dependencies"
  },
  "devDependencies": {
    "@iconify-json/devicon": "^1.2.14",
    "@iconify-json/ph": "^1.2.2",
    "@sveltejs/adapter-cloudflare": "5.0.2",
    "@sveltejs/kit": "^2.17.1",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/eslint": "^9.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.1",
    "bits-ui": "1.0.0-next.94",
    "clsx": "^2.1.1",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "formsnap": "2.0.0",
    "globals": "^15.14.0",
    "knip": "^5.44.0",
    "lucide-svelte": "^0.475.0",
    "mode-watcher": "^0.5.1",
    "prettier": "^3.5.0",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "supabase": "^2.12.0",
    "supabase-to-zod": "^1.0.7",
    "svelte": "^5.19.9",
    "svelte-check": "^4.1.4",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.23.1",
    "tailwind-merge": "^3.0.1",
