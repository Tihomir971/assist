    "html5-qrcode": "^2.3.8",
    "ky": "^1.8.1",
    "luxon": "^3.6.1",
    "match-sorter": "^8.0.2",
    "mathjs": "^14.5.2",
    "mode-watcher": "^1.0.7",
    "svelte-sonner": "^1.0.4",
    "sveltekit-superforms": "^2.26.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "4.1.8",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.25.51"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "supabase",
      "workerd"
    ]
  }
