{
  "name": "assist",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": "22.13.1"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "update-deps": "pnpx npm-check-updates -i --format group --install always",
    "update-next": "pnpx npm-check-updates -i --format group --install always --target \"@next\"",
    "supabase-to-zod": "pnpm exec supabase-to-zod --input \"./src/lib/types/supabase/database.types.ts\" --output \"./src/lib/types/supabase/supabase-zod-schemas.ts\"",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "knip": "knip"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/js": "^9.22.0",
    "@iconify-json/devicon": "^1.2.20",
    "@iconify-json/ph": "^1.2.2",
    "@sveltejs/adapter-cloudflare": "5.1.0",
    "@sveltejs/kit": "^2.20.0",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@types/eslint": "^9.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.10",
    "bits-ui": "1.3.13",
    "clsx": "^2.1.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-svelte": "^3.3.0",
    "formsnap": "2.0.0",
    "globals": "^16.0.0",
    "knip": "^5.46.0",
    "lucide-svelte": "^0.482.0",
    "mode-watcher": "^0.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "supabase": "^2.19.7",
    "supabase-to-zod": "^1.0.7",
    "svelte": "^5.23.1",
    "svelte-check": "^4.1.5",
    "svelte-sonner": "^0.3.28",
    "sveltekit-superforms": "^2.24.0",
    "tailwind-merge": "^3.0.2",
    "tailwind-variants": "^1.0.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "unplugin-icons": "^22.1.0",
    "vaul-svelte": "1.0.0-next.6",
    "vite": "^6.2.2",
    "wrangler": "^4.0.0",
    "zod": "^3.24.2"
  },
  "dependencies": {
    "@fontsource-variable/nunito-sans": "^5.2.5",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/vite": "4.0.14",
    "@tanstack/table-core": "^8.21.2",
    "echarts": "^5.6.0",
    "html5-qrcode": "^2.3.8",
    "ky": "^1.7.5",
    "luxon": "^3.5.0",
    "melt": "^0.17.4",
    "tailwindcss": "4.0.14",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "supabase",
      "workerd"
    ]
  }
}