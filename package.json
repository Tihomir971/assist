{
  "name": "assist",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "pnpm run build && wrangler dev",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "update-deps": "pnpx npm-check-updates -i --format group --install always",
    "update-next": "pnpx npm-check-updates -i --format group --install always --target \"@next\"",
    "supazod": "pnpm exec supazod -i \"./src/lib/types/supabase.types.ts\" -o \"./src/lib/types/supabase.zod.schemas.ts\"  -t \"./src/lib/types/supabase.zod.schemas.d.ts\" -s public",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "knip": "knip",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
    "supabase-to-zod": "pnpm exec supabase-to-zod --input \"./src/lib/types/supabase.types.ts\" --output \"./src/lib/types/supabase.zod.schemas.ts\""
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.30.1",
    "@iconify-json/devicon": "^1.2.29",
    "@iconify-json/ph": "^1.2.2",
    "@internationalized/date": "^3.8.2",
    "@lucide/svelte": "^0.525.0",
    "@sveltejs/adapter-cloudflare": "7.0.4",
    "@sveltejs/kit": "^2.22.2",
    "@sveltejs/vite-plugin-svelte": "5.1.0",
    "@types/eslint": "^9.6.1",
    "@types/file-saver": "^2.0.7",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.10",
    "bits-ui": "2.8.10",
    "clsx": "^2.1.1",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-svelte": "^3.10.1",
    "globals": "^16.2.0",
    "knip": "^5.61.3",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "supabase": "^2.26.9",
    "supabase-to-zod": "^1.0.7",
    "supazod": "^2.0.0",
    "svelte": "^5.34.9",
    "svelte-check": "^4.2.2",
    "tailwind-variants": "^1.0.0",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "unplugin-icons": "^22.1.0",
    "vaul-svelte": "1.0.0-next.7",
    "vite": "^6.3.5",
    "vite-plugin-devtools-json": "^0.2.1",
    "wrangler": "^4.22.0"
  },
  "dependencies": {
    "@fontsource-variable/fira-code": "^5.2.6",
    "@fontsource-variable/merriweather": "^5.2.4",
    "@fontsource-variable/nunito": "^5.2.6",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.2",
    "@tailwindcss/vite": "4.1.11",
    "@tanstack/svelte-table": "npm:tanstack-table-8-svelte-5@^0.1",
    "@tanstack/table-core": "^8.21.3",
    "@tihomir971/assist-shared": "^1.0.5",
    "@zag-js/avatar": "^1.17.4",
    "@zag-js/checkbox": "^1.17.4",
    "@zag-js/collection": "^1.17.4",
    "@zag-js/combobox": "^1.17.4",
    "@zag-js/file-upload": "^1.17.4",
    "@zag-js/menu": "^1.17.4",
    "@zag-js/number-input": "^1.17.4",
    "@zag-js/select": "^1.17.4",
    "@zag-js/svelte": "^1.17.4",
    "@zag-js/switch": "^1.17.4",
    "@zag-js/tree-view": "^1.17.4",
    "echarts": "^5.6.0",
    "fast-xml-parser": "^5.2.5",
    "formsnap": "^2.0.1",
    "html5-qrcode": "^2.3.8",
    "ky": "^1.8.1",
    "luxon": "^3.6.1",
    "match-sorter": "^8.0.3",
    "mathjs": "^14.5.2",
    "mode-watcher": "^1.1.0",
    "svelte-sonner": "^1.0.5",
    "sveltekit-superforms": "^2.27.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "4.1.11",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.25.67"
  },
  "pnpm": {
    "overrides": {
      "@tanstack/svelte-table": "npm:tanstack-table-8-svelte-5@^0.1"
    },
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "supabase",
      "workerd"
    ]
  }
}