{
	"inputs": [
		{
			"id": "input1",
			"description": "Enter a prompt string",
			"type": "promptString"
		}
	],
	"servers": {
		"fetch": {
			"command": "uvx",
			"args": ["mcp-server-fetch"]
		},
		"ark-ui": {
			"command": "npx",
			"args": ["-y", "@ark-ui/mcp"]
		},
		"chrome-devtools": {
			"command": "npx",
			"args": ["chrome-devtools-mcp@latest"]
		},
		"svelte": {
			"command": "npx",
			"args": ["-y", "@sveltejs/mcp"]
		}
	}
}
